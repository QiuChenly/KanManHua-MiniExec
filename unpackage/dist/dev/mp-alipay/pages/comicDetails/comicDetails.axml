<scroll-view class="content"><q-bar vue-id="658b6636-1" bgcolor="{{barcolor}}" barTit="{{barTit}}" onVueInit="__l"></q-bar><view class="bg" style="{{'position:'+('relative')+';'+('height:'+(barHeight+'px')+';')}}"><image class="head" mode="aspectFill" src="{{barImg}}"></image><view style="position:absolute;top:0;left:0;width:100%;height:100%;background-color:rgba(0,0,0,.4);" class="_div"></view><view class="detail" style="{{'width:'+('100%')+';'}}"><view class="_div"><block a:for="{{comicdata.comic_type_new}}" a:for-item="item" a:for-index="index" a:key="index"><text class="type-item">{{''+item.name+''}}</text></block></view><view style="{{'margin-top:'+('5px')+';'}}" class="_div"><text style="{{'font-size:'+('16px')+';'}}">{{comicdata.comic_name}}</text><text style="{{'font-weight:'+('100;')+';'+('font-size:'+('13px')+';')+('margin-left:'+('10px')+';')}}">{{comicdata.comic_media}}</text></view><view class="_div"><text style="{{'font-size:'+('13px')+';'}}">{{"人气:"+$root.g0+"亿"}}</text></view></view></view><q-tab vue-id="658b6636-2" list="{{tablist}}" dotActiveColor="{{dotcolor}}" currentindex="{{tabcurrent}}" data-event-opts="{{[['^changeItem',[['itemchange']]]]}}" onChangeItem="__e" onVueInit="__l"></q-tab><swiper style="height:100vh;" duration="{{300}}" current="{{tabcurrent}}" data-event-opts="{{[['change',[['swiperchange',['$event']]]]]}}" onChange="__e"><swiper-item class="comic-desc"><comicdesc vue-id="658b6636-3" onVueInit="__l"></comicdesc></swiper-item><swiper-item class="comic-list"><view>b</view></swiper-item></swiper></scroll-view>